<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>@Model.ChapterTitle</title>
</head>
<body>
    <h1>@Model.ChapterTitle</h1>
    @foreach (var line in Model.lines)
    {
        if (string.IsNullOrWhiteSpace(line))
        {
            <br></br>
        }
        else
        {
            <p>@line</p>
        }
    }
</body>
</html>